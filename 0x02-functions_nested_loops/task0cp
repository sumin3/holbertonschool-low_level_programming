#include <stdio.h>
/**
 * main - print "Holberton" use putchar
 * Return: 0
 */
int main(void)
{
	char message = 'b';
	int count =0;

	putchar('H');
	while (message >= 'b'&& message <= 't' && count <8)
	{
		if (message == 'b' && count == 0)
			message = 'o';
		else if (message == 'o' && count == 1)
			message = 'l';
		else if (message == 'l')
			message = 'b';
		else if (message == 'b')
			message= 'e';
		else if (message == 'e')
			message= 'r';
		else if (message == 'r')
			message= 't';
		else if (message == 't')
			message= 'o';
		else if (message == 'o')
			message= 'n';
		count++;
		putchar(message);
	}
	putchar('\n');
	return (0);
}
